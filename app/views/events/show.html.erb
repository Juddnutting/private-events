<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        Event created by <%= @event.creator.name %> 
      </h1>
      <div class="col-md-8">
            <h3><%= @event.title %> Attendees</h3>
            <ul>
            <% @event.attendees.each do |attendee|  %>
            <li><%= attendee.name %></li>
            <%end %>
            </ul>  
            
        </div>
      
      
    </section>
  </aside>
</div>